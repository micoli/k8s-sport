{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.0.8",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "friendsofphp/php-cs-fixer": "^2.14",
    "guzzlehttp/guzzle": "^6.3",
    "infection/infection": "^0.12.2",
    "phpunit/phpunit": "^8.0",
    "ramsey/uuid": "^3.8",
    "symfony/console": "3.4.*",
    "symfony/dotenv": "3.4.*",
    "symfony/flex": "^1.1",
    "symfony/framework-bundle": "3.4.*",
    "symfony/lock": "3.4.*",
    "symfony/monolog-bundle": "^3.3",
    "symfony/twig-bundle": "3.4.*",
    "symfony/yaml": "3.4.*",
    "cboden/ratchet": "dev-master",
    "psr/log": "~1.0",
    "monolog/monolog": "~1.0",
    "cujojs/when": "2.7.*",
    "tavendo/autobahnjs": "0.8.*",
    "gimite/web-socket-js": "dev-master"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "gimite/web-socket-js",
        "type": "library",
        "version": "dev-master",
        "source": {
          "url": "https://github.com/gimite/web-socket-js",
          "type": "git",
          "reference": "master"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "tavendo/AutobahnJS",
        "type": "library",
        "version": "0.8.2",
        "source": {
          "url": "https://github.com/tavendo/AutobahnJS.git",
          "type": "git",
          "reference": "v0.8.2"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "cujojs/when",
        "type": "library",
        "version": "2.7.1",
        "source": {
          "url": "https://github.com/cujojs/when.git",
          "type": "git",
          "reference": "2.7.1"
        }
      }
    }
  ],
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "3.4.*"
    }
  }
}
